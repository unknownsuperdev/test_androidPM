<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/primary_white">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/report"
        style="@style/Body_SemiBold_16_Black"
        android:layout_width="@dimen/space_0"
        android:layout_height="@dimen/space_48"
        android:background="@drawable/bg_selector_dark_and_white"
        android:clickable="true"
        android:drawableStart="@drawable/ic_report_flag"
        android:drawablePadding="@dimen/space_12"
        android:focusable="true"
        android:gravity="center_vertical"
        android:paddingStart="@dimen/space_12"
        android:text="@string/report"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="RtlSymmetry" />

</androidx.constraintlayout.widget.ConstraintLayout>