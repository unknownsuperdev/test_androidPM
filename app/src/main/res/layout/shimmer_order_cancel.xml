<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="@dimen/space_16">

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/message_line_1"
        android:layout_width="0dp"
        android:layout_height="@dimen/space_216"
        android:background="@drawable/shape_gray_rectangle_8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/message_line_2"
        android:layout_width="@dimen/space_104"
        android:layout_height="@dimen/space_24"
        android:layout_marginTop="@dimen/space_32"
        android:background="@drawable/shape_gray_rectangle_8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/message_line_1" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/line_3"
        android:layout_width="@dimen/space_0"
        android:layout_height="@dimen/space_24"
        android:layout_marginTop="@dimen/space_16"
        android:layout_marginEnd="@dimen/space_68"
        android:background="@drawable/shape_gray_rectangle_8"
        app:layout_constraintEnd_toStartOf="@+id/line_4"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/message_line_2" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/line_4"
        android:layout_width="@dimen/space_24"
        android:layout_height="@dimen/space_24"
        android:background="@drawable/shape_gray_rectangle_8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/line_3" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/line_5"
        android:layout_width="@dimen/space_0"
        android:layout_height="@dimen/space_24"
        android:layout_marginTop="@dimen/space_16"
        android:layout_marginEnd="@dimen/space_68"
        android:background="@drawable/shape_gray_rectangle_8"
        app:layout_constraintEnd_toStartOf="@+id/line_6"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line_3" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/line_6"
        android:layout_width="@dimen/space_24"
        android:layout_height="@dimen/space_24"
        android:background="@drawable/shape_gray_rectangle_8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/line_5" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/line_7"
        android:layout_width="@dimen/space_0"
        android:layout_height="@dimen/space_24"
        android:layout_marginTop="@dimen/space_16"
        android:layout_marginEnd="@dimen/space_68"
        android:background="@drawable/shape_gray_rectangle_8"
        app:layout_constraintEnd_toStartOf="@+id/line_8"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line_5" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/line_8"
        android:layout_width="@dimen/space_24"
        android:layout_height="@dimen/space_24"
        android:background="@drawable/shape_gray_rectangle_8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/line_7" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/line_9"
        android:layout_width="@dimen/space_0"
        android:layout_height="@dimen/space_24"
        android:layout_marginTop="@dimen/space_16"
        android:layout_marginEnd="@dimen/space_68"
        android:background="@drawable/shape_gray_rectangle_8"
        app:layout_constraintEnd_toStartOf="@+id/line_10"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/line_7" />

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/line_10"
        android:layout_width="@dimen/space_24"
        android:layout_height="@dimen/space_24"
        android:background="@drawable/shape_gray_rectangle_8"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="@+id/line_9" />

</androidx.constraintlayout.widget.ConstraintLayout>
