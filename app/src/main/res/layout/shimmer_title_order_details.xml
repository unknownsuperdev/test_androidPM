<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <View
        android:id="@+id/vcdf"
        android:layout_width="0dp"
        android:layout_height="@dimen/space_20"
        android:layout_marginStart="@dimen/space_16"
        android:layout_marginEnd="190dp"
        android:layout_marginBottom="@dimen/space_8"
        android:background="@drawable/order_page_rect_shimmer"
        app:layout_constraintBottom_toTopOf="@+id/shimmerToolbarSecondView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/shimmerToolbarSecondView"
        android:layout_width="0dp"
        android:layout_height="@dimen/space_20"
        android:layout_marginStart="@dimen/space_16"
        android:layout_marginEnd="29dp"
        android:layout_marginBottom="@dimen/space_8"
        android:background="@drawable/order_page_rect_shimmer"
        app:layout_constraintBottom_toTopOf="@+id/shimmerToolbarFirstView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <View
        android:id="@+id/shimmerToolbarFirstView"
        android:layout_width="0dp"
        android:layout_height="@dimen/space_20"
        android:layout_marginStart="@dimen/space_16"
        android:layout_marginEnd="104dp"
        android:layout_marginBottom="@dimen/space_40"
        android:background="@drawable/order_page_rect_shimmer"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>